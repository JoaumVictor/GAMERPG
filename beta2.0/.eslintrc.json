{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "jsx-quotes": ["error", "prefer-single"],
    "no-alert": "off",
    "no-console": "off",
    "import/no-unresolved": 0,
    "import/extensions": [".js", ".jsx", ".json", ".ts", ".tsx"]
  }
}
